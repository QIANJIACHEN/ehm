<%@ page contentType="text/html;charset=UTF-8" language="java" %>

<div class="modal fade" id="recorddataplane-add-modal" tabindex="-1" role="dialog" aria-labelledby="recorddataplane-info-modal-label">
    <div class="modal-dialog" role="document">
        <div class="box box-primary">
            <div class="box-header with-border">
                <h3 class="box-title">飞机属性更变信息</h3>
            </div>
            <form id="form-recorddataplaneadd" method="post" enctype="multipart/form-data">
                <div class="box-body">
                        
                    
                    <div class="form-group">
                        <label for="text-username">TAIL</label>
                        <input type="tel" class="form-control" id="addtail"
                               placeholder="TAIL">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">基地</label>
                        <select class="form-control" id="addduty">
                        	<option value="0">请选择</option>
                        	<option value="1">测试用的</option>
                        </select>
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">推力</label>
                        <input type="text" class="form-control" id="addratingPlane"
                               placeholder="推力">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">构型</label>
                        <input type="text" class="form-control" id="addconfigPlane"
                               placeholder="构型">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">原始的发动机编码</label>
                        <input type="text" class="form-control" id="addegsnOriginal"
                               placeholder="原始的发动机编码">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">型号1级</label>
                        <input type="text" class="form-control" id="addseries"
                               placeholder="型号1级">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">型号2级</label>
                        <input type="text" class="form-control" id="addtype"
                               placeholder="型号2级">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">型号3级</label>
                        <input type="text" class="form-control" id="addmodel"
                               placeholder="型号3级">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">属性</label>
                        <input type="text" class="form-control" id="addattribute"
                               placeholder="属性">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">ETOPS</label>
                        <input type="text" class="form-control" id="addetops"
                               placeholder="ETOPS">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">高高原</label>
                        <select class="form-control" id="addhHighLand">
                        	<option value="1">是</option>
                        	<option value="0">否</option>
                        </select>
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">VARIABLE</label>
                        <input type="text" class="form-control" id="addvariable"
                               placeholder="VARIABLE">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">MSN</label>
                        <input type="text" class="form-control" id="addserial"
                               placeholder="MSN">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">LINE</label>
                        <input type="text" class="form-control" id="addline"
                               placeholder="LINE">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">MANUAL</label>
                        <input type="text" class="form-control" id="addmanual"
                               placeholder="MANUAL">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">航空公司（品牌）</label>
                        <input type="text" class="form-control" id="addoperater"
                               placeholder="航空公司（品牌）">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">所有人</label>
                        <input type="text" class="form-control" id="addowner"
                               placeholder="所有人">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">备注</label>
                        <input type="text" class="form-control" id="addremark"
                               placeholder="备注">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">引进方式</label>
                        <input type="text" class="form-control" id="addimportWay"
                               placeholder="引进方式">
                    </div>
                    <!-- 
                    <div class="form-group">
                        <label for="text-nickname">录入人</label>
                        <input type="text" class="form-control" id="adduserId"
                               placeholder="录入人">
                    </div>
                     -->
                </div>
                <div class="box-footer">
                    <button type="button" class="btn btn-primary pull-right" onclick="addrecorddataplane()">保存</button>
                </div>
            </form>
        </div>
    </div>
</div>

<%@ page contentType="text/html;charset=UTF-8" language="java" %>

<div class="modal fade" id="fleeteng-info-modal" tabindex="-1" role="dialog" aria-labelledby="fleeteng-info-modal-label">
    <div class="modal-dialog" role="document">
        <div class="box box-primary">
            <div class="box-header with-border">
                <h3 class="box-title">发动机备发信息</h3>
            </div>
            <form id="form-fleetenginfo" method="post" enctype="multipart/form-data">
                <div class="box-body">
                   <input type="hidden" id="editid" value="">
                    <div class="form-group">
                        <label for="text-username">TAIL</label>
                        <select class="form-control planeId" id="edittail">
                        	<option value="0">请选择</option>
                        </select>
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">ENG_SN</label>
                        <select class="form-control engId" id="editengSn">
                        	<option value="0">请选择</option>
                        </select>
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">发动机位置</label>
                        <select class="form-control" id="editengPosition">
                        	<option value="1">左</option>
                        	<option value="2">右</option>
                        </select>
                    </div>
                </div>
                <div class="box-footer">
                    <button type="button" class="btn btn-primary pull-right" onclick="editfleeteng()">保存</button>
                </div>
            </form>
        </div>
    </div>
</div>

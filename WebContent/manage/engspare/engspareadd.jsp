<%@ page contentType="text/html;charset=UTF-8" language="java" %>

<div class="modal fade" id="waterwash-add-modal" tabindex="-1" role="dialog" aria-labelledby="user-info-modal-label">
    <div class="modal-dialog" role="document">
        <div class="box box-primary">
            <div class="box-header with-border">
                <h3 class="box-title">发动机备发信息</h3>
            </div>
            <form id="form-userinfo" method="post" enctype="multipart/form-data">
                <div class="box-body">
                   
                    <div class="form-group">
                        <label for="text-username">发动机号</label>
                        <input type="tel" class="form-control" id="addengSn"
                               placeholder="发动机号">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">删除日期</label>
                        <input type="date" class="form-control" id="addremoveDate"
                               placeholder="删除日期">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">删除原因</label>
                        <input type="text" class="form-control" id="addremovalCause"
                               placeholder="删除原因">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">状态</label>
                        <input type="text" class="form-control" id="addcondition_"
                               placeholder="状态">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">工厂</label>
                        <input type="text" class="form-control" id="addfactory"
                               placeholder="工厂">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">到达日期</label>
                        <input type="date" class="form-control" id="addarrived"
                               placeholder="到达日期">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">就位日期</label>
                        <input type="date" class="form-control" id="addinduction"
                               placeholder="就位日期">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">完成计划时间</label>
                        <input type="date" class="form-control" id="addcompletePlan"
                               placeholder="完成计划时间">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">完成时间</label>
                        <input type="date" class="form-control form_datetime" id="addcomplete"
                               placeholder="完成时间">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">SHIP</label>
                        <input type="text" class="form-control" id="addship"
                               placeholder="SHIP">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">TAT</label>
                        <input type="text" class="form-control" id="addtat"
                               placeholder="TAT">
                    </div>
                </div>
                <div class="box-footer">
                    <button type="button" class="btn btn-primary pull-right" onclick="addwaterwash()">保存</button>
                </div>
            </form>
        </div>
    </div>
</div>

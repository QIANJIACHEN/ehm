<%@ page contentType="text/html;charset=UTF-8" language="java" %>

<div class="modal fade" id="pwf-info-modal" tabindex="-1" role="dialog" aria-labelledby="pwf-info-modal-label">
    <div class="modal-dialog" role="document">
        <div class="box box-primary">
            <div class="box-header with-border">
                <h3 class="box-title">飞机航班关联信息</h3>
            </div>
            <form id="form-info" method="post" enctype="multipart/form-data">
                <div class="box-body">
                	 <div class="form-group">
                        <label for="text-nickname">机号</label>
                        <select id="editplaneId" class="form-control" style="width: 100%">
		                </select>
                    </div>
                	<div class="form-group">
                        <label for="text-nickname">航班</label>
                        <select id="editflightId" class="form-control" style="width: 100%">
		                </select>
                    </div>
                </div>
                <div class="box-footer" style="text-align:right">
                    <button type="button" class="btn btn-primary" onclick="editpwf()">保存</button>
                    <button type="button" class="btn btn-default"  data-dismiss="modal" >取消</button>
                </div>
            </form>
        </div>
    </div>
</div>

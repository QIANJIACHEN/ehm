<%@ page contentType="text/html;charset=UTF-8" language="java" %>

<div class="modal fade" id="dataidplug-info-modal" tabindex="-1" role="dialog" aria-labelledby="dataidplug-info-modal-label">
    <div class="modal-dialog" role="document">
        <div class="box box-primary">
            <div class="box-header with-border">
                <h3 class="box-title">IDPLUG数据</h3>
            </div>
            <form id="form-info" method="post" enctype="multipart/form-data">
                <div class="box-body">
                    <div class="form-group">
                        <label for="text-username">发动机SN</label>
                        <select id="editengId" class="form-control" style="width: 100%">
		                </select>
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">转速</label>
                        <input type="text" class="form-control" id="editrating"
                               placeholder="转速">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">飞机型号</label>
                        <input type="text" class="form-control" id="editplaneType"
                               placeholder="飞机型号">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">IDPLUGPN</label>
                        <input type="text" class="form-control" id="editidPlugPn"
                               placeholder="IDPLUG_PN">
                    </div>
                </div>
                <div class="box-footer" style="text-align: right;">
                    <button type="button" class="btn btn-primary" onclick="editdataidplug()">保存</button>
                    <button type="button" class="btn btn-default " data-dismiss="modal">取消</button>
                </div>
            </form>
        </div>
    </div>
</div>

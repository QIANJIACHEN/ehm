<script>
    var base = "${pageContext.request.contextPath}";
    var storeUrl = null;
    function jumpPage(url) {
    	console.log(11);
        if (storeUrl == url) {
            return;
        }
        document.getElementById("myiframe").src = base + url;
        storeUrl = url;
    }
    var index;
	$(".treeview-menu li a").each(function(){
		$(this).on('click',function(index){
			var url = $(this).attr('tiao_url');
			var $ele=$(this);
			var tex=$(this).text();
			if($ele.attr('data') == 1){
				$ele.attr('data',2);
				$(".uls").append("<li onclick=\"jumpPage('"+url+"')\" style='cursor: pointer;float:left;padding:8px 25px;float:left;overflow:hidden; margin-left:10px;margin-bottom:10px;border:1px solid #969595;'>"+"<a style='position: relative;color:#969595;'>"+tex+"<span class='a' style='position:absolute;top:-14px;right:-20px;font-size:20px;cursor: pointer'>x</span>"+"</a>"+"</li>");
			}
			$(".a").click(function(e){
				console.log($ele);
				$ele.attr('data',1);
				$(this).parent().parent().remove();
			})
			/*  if(!$ele.data('add')) {
				 console.log($ele.data('add'));
			    $ele.data('add',true);
			    $(".uls").append("<li onclick=\"jumpPage('"+url+"')\" style='cursor: pointer;float:left;padding:8px 25px;float:left;overflow:hidden; margin-left:10px;margin-bottom:10px;border:1px solid #969595;'>"+"<a style='position: relative;color:#969595;'>"+tex+"<span class='a' style='position:absolute;top:-14px;right:-20px;font-size:20px;cursor: pointer'>x</span>"+"</a>"+"</li>");
			  } */
			
		})
		})
		
		
		
    
</script>

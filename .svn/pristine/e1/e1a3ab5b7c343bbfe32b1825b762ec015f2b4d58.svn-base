<%@ page contentType="text/html;charset=UTF-8" language="java" %>

<div class="modal fade" id="department-add-modal" tabindex="-1" role="dialog">
    <div class="modal-dialog" role="document">
        <div class="box box-primary">
            <div class="box-header with-border">
                <h3 class="box-title">添加部门</h3>
            </div>
            <form method="post">
                <div class="box-body">
                    <div class="form-group">
                        <label for="text-jd">所属基地</label>
                        <input type="text" class="form-control" id="text-jd"
                               placeholder="所属基地">
                    </div>
                    <div class="form-group">
                        <label for="text-teamname">部门名称</label>
                        <input type="text" class="form-control" id="text-teamname"
                               placeholder="部门名称">
                    </div>
                    <div class="form-group">
                        <label for="text-leadername">部门领导</label>
                        <input type="text" class="form-control" id="text-leadername"
                               placeholder="姓名">
                    </div>
                    <div class="form-group">
                        <label for="text-phone">联系方式</label>
                        <input type="text" class="form-control" id="text-phone"
                               placeholder="联系方式">
                    </div>
                    <div class="form-group">
                        <label for="text-location">所在地址</label>
                        <input type="text" class="form-control" id="text-location"
                               placeholder="地址">
                    </div>
                </div>
                <div class="box-footer" style="text-align:right">
                    <button type="button" class="btn btn-primary">确定</button>
                    <button type="button" class="btn btn-default"  data-dismiss="modal">取消</button>
                </div>
            </form>
        </div>
    </div>
</div>
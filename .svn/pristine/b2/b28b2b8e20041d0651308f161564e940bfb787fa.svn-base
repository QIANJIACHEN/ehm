<%@ page contentType="text/html;charset=UTF-8" language="java" %>

<div class="modal fade" id="ruleinspection-add-modal" tabindex="-1" role="dialog" aria-labelledby="user-info-modal-label">
    <div class="modal-dialog" role="document">
        <div class="box box-primary">
            <div class="box-header with-border">
                <h3 class="box-title">规则参数信息</h3>
            </div>
            <form id="form-userinfo" method="post" enctype="multipart/form-data">
                <div class="box-body">
                        
                    <input type="hidden" id="addid">
                    <div class="form-group">
                        <label for="text-username">上级ID</label>
                        <input type="tel" class="form-control" id="addparentId"
                               placeholder="上级ID">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">类型</label>
                        <input type="text" class="form-control" id="addtype"
                               placeholder="类型">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">值</label>
                        <input type="text" class="form-control" id="addval"
                               placeholder="值">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">注释</label>
                        <input type="text" class="form-control" id="addannotation"
                               placeholder="注释">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">是否加入计算</label>
                        <input type="text" class="form-control" id="addisCalculate"
                               placeholder="是否加入计算">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">是否显示在报告中</label>
                        <input type="text" class="form-control" id="addisDisplay"
                               placeholder="显示">
                    </div>
                </div>
                <div class="box-footer">
                    <button type="button" class="btn btn-primary pull-right" onclick="addruleinspection()">保存</button>
                </div>
            </form>
        </div>
    </div>
</div>

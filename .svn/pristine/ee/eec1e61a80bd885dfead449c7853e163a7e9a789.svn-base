<script type="text/javascript">

	// 确定按钮点击事件
	$('#save').click(function(){
		// 判断表单内容
		var flag=true;
		var t = $('#form').serializeArray();
		$.each(t, function() {
			// 为什么是this，而t为什么不行
			if(this.value == null || this.value.trim() == ''){
			    flag=false;
			}
		});
		if(flag){
			$.ajax({  
			    type: 'POST',  
			    cache: false, // 是否有缓存
			    url: "${pageContext.request.contextPath}/manage/calculate/save.do",  
			    dataType: 'json', 
			    data: $('#form').serialize(),  
			    async: true,  // 是否异步
			    success: function(data) {  
			        //接收后台返回的结果  
			        if(data.code != 101){
			    		alert(data.msg); 
			        }
			    }
			});  
		}else{
			 alert('参数不完整！');
		}
	});
	
	// addcomputation
	function addcomputation() {
		var type = $("#type").val();
		var calculationType = $("#calculationType").val();
		var calculationRange= $("#calculationRange").val();
		var rangeType = $("#rangeType").val();
		var flightPhase = $("#flightPhase").val();
		var max = $("#max").val();
		var min = $("#min").val();
		var equal = $("#equal").val();
		var unequal = $("#unequal").val();
		$.post(
		    "${pageContext.request.contextPath}/manage/calculate/savecomputation.do",
		    {
		        "type": type,
		        "calculationType": calculationType,
		        "calculationRange": calculationRange,
		        "rangeType": rangeType,
		        "flightPhase": flightPhase,
		        "max": max,
		        "min": min,
		        "equal": equal,
		        "unequal": unequal
		    },
		    function (response) {
		    	if(response.code != 101){
		    		alert(response.msg);
		    	}
		    	 $('#add-modal').modal('hide');
		    },
		    "json"
		);
	}
	
	 // 清除弹窗原数据
	 $("#add-modal").on("hidden.bs.modal", function() {
	     $('#reset').click();
	 });
	
</script>

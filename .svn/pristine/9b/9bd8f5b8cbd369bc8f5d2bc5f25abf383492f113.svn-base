
DROP TABLE IF EXISTS `auth_access`;
CREATE TABLE `auth_access` (
  `ID` varchar(64) NOT NULL,
  `MODULE` varchar(64) NOT NULL,
  `TYPE` varchar(64) NOT NULL,
  `NAME` varchar(64) NOT NULL,
  `PARAM` varchar(64) NOT NULL,
  `TITLE` varchar(64) NOT NULL,
  `STATUS` varchar(64) NOT NULL,
  `CONDITION` varchar(64) NOT NULL,
  PRIMARY KEY  (`ID`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;


DROP TABLE IF EXISTS `auth_rule`;
CREATE TABLE `auth_rule` (
  `ROLE_ID` varchar(64) NOT NULL,
  `RULE_NAME` varchar(64) NOT NULL,
  `TYPE` varchar(64) NOT NULL,
  PRIMARY KEY  (`ROLE_ID`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;


DROP TABLE IF EXISTS `data_eng`;
CREATE TABLE `data_eng` (
  `ENGID` varchar(64) NOT NULL,
  `ENG_SN` varchar(64) NOT NULL,
  `ENG_SN_SAP` varchar(64) NOT NULL,
  `ENG_SERIES` varchar(64) NOT NULL,
  `ENG_TYPE` varchar(64) NOT NULL,
  `ENG_MODEL` varchar(64) NOT NULL,
  `ENG_CONFIG` varchar(64) NOT NULL,
  `DELIVERY_MODE` varchar(64) NOT NULL,
  `OWNER` varchar(64) NOT NULL,
  `CODE_CONFIG` varchar(64) NOT NULL,
  `ENG_CONFIGRATION` varchar(64) NOT NULL,
  `CTIME` varchar(64) NOT NULL,
  PRIMARY KEY  (`ENGID`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;


DROP TABLE IF EXISTS `data_plane`;
CREATE TABLE `data_plane` (
  `PLANEID` varchar(64) NOT NULL,
  `TAIL` varchar(64) NOT NULL,
  `DUTY` varchar(64) NOT NULL,
  `RATING_PLANE` varchar(64) NOT NULL,
  `CONFIG_PLANE` varchar(64) NOT NULL,
  `ENG_SN_ORIGINAL` varchar(64) NOT NULL,
  `SERIES` varchar(64) NOT NULL,
  `TYPE` varchar(64) NOT NULL,
  `MODEL` varchar(64) NOT NULL,
  `ATTRIBUTE` varchar(64) NOT NULL,
  `ETOPS` varchar(64) NOT NULL,
  `H_HIGHLAND` varchar(64) NOT NULL,
  `VARIABLE` varchar(64) NOT NULL,
  `SERIAL` varchar(64) NOT NULL,
  `LINE` varchar(64) NOT NULL,
  `MANUAL` varchar(64) NOT NULL,
  `OPERATER` varchar(64) NOT NULL,
  `OWNER` varchar(64) NOT NULL,
  `REMARK` varchar(64) NOT NULL,
  `IMPORT_WAY` varchar(64) NOT NULL,
  `CTIME` varchar(64) NOT NULL,
  PRIMARY KEY  (`PLANEID`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;


DROP TABLE IF EXISTS `role`;
CREATE TABLE `role` (
  `ID` varchar(64) NOT NULL,
  `NAME` varchar(64) NOT NULL,
  `PID` varchar(64) NOT NULL,
  `STATUS` varchar(64) NOT NULL,
  `REMARK` varchar(64) NOT NULL,
  `CTIME` varchar(64) NOT NULL,
  `UTIME` varchar(64) NOT NULL,
  `LISTORDER` varchar(64) NOT NULL,
  PRIMARY KEY  (`ID`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;


DROP TABLE IF EXISTS `role_user`;
CREATE TABLE `role_user` (
  `ROLE_ID` varchar(64) NOT NULL,
  `USER_ID` varchar(64) NOT NULL
) ENGINE=InnoDB DEFAULT CHARSET=utf8;


DROP TABLE IF EXISTS `user`;
CREATE TABLE `user` (
  `USERID` varchar(64) NOT NULL,
  `USERNAME` varchar(64) NOT NULL,
  `REALNAME` varchar(64) NOT NULL,
  `PASSWORD` varchar(64) NOT NULL,
  `POSITION` varchar(64) NOT NULL,
  `ROLE` varchar(64) NOT NULL,
  `PHONE` int(11) NOT NULL,
  `CTIME` varchar(64) NOT NULL,
  `DUTY` varchar(64) NOT NULL,
  `DEPARTMENT_ID` varchar(64) NOT NULL,
  PRIMARY KEY  (`USERID`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

DROP TABLE IF EXISTS `hptn_activity`;
CREATE TABLE `hptn_activity` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `ENG_SN` varchar(50) DEFAULT NULL,
  `HPTN_PN` varchar(50) DEFAULT NULL,
  `HPTN_SN` varchar(50) DEFAULT NULL,
  `ENG_PN` varchar(50) DEFAULT NULL,
  `ON_DATE` tinyint(1) DEFAULT NULL,
  `ON_ETSN` tinyint(1) DEFAULT NULL,
  `ON_ECSN` tinyint(1) DEFAULT NULL,
  `ON_PTSN` tinyint(1) DEFAULT NULL,
  `ON_PCSN` tinyint(1) DEFAULT NULL,
  `LIMIT_SPECIAL` varbinary(50) DEFAULT NULL,
  `CODE` varchar(50) DEFAULT NULL,
  `REMARK` varchar(512) DEFAULT NULL COMMENT '备注',
  PRIMARY KEY (`id`)
) ENGINE=MyISAM AUTO_INCREMENT=9 DEFAULT CHARSET=utf8;

DROP TABLE IF EXISTS `hptn_history`;
CREATE TABLE `hptn_history` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `ENG_SN_USED` varchar(50) DEFAULT NULL,
  `HPTN_PN` varchar(50) DEFAULT NULL,
  `HPTN_SN` varchar(50) DEFAULT NULL,
  `ENG_PN` varchar(50) DEFAULT NULL,
  `ON_DATE` tinyint(1) DEFAULT NULL,
  `ON_ETSN` tinyint(1) DEFAULT NULL,
  `ON_ECSN` tinyint(1) DEFAULT NULL,
  `ON_PTSN` tinyint(1) DEFAULT NULL,
  `ON_PCSN` tinyint(1) DEFAULT NULL,
  `OFF_DATE` tinyint(1) DEFAULT NULL,
  `OFF_ETSN` tinyint(1) DEFAULT NULL,
  `OFF_ECSN` tinyint(1) DEFAULT NULL,
  `OFF_PTSN` tinyint(1) DEFAULT NULL,
  `OFF_PCSN` tinyint(1) DEFAULT NULL,
  `LIMIT_SPECIAL` varchar(50) DEFAULT NULL,
  `CODE` varchar(50) DEFAULT NULL,
  `REMARK` varchar(512) DEFAULT NULL,
  PRIMARY KEY (`id`)
) ENGINE=MyISAM AUTO_INCREMENT=20 DEFAULT CHARSET=utf8;

DROP TABLE IF EXISTS `lptn1_activity`;
CREATE TABLE `lptn1_activity` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `ENG_SN` varchar(50) DEFAULT NULL,
  `LPTN1_PN` varchar(50) DEFAULT NULL,
  `LPTN1_SN` varchar(50) DEFAULT NULL,
  `ENG_PN` varchar(50) DEFAULT NULL,
  `ON_DATE` int(11) DEFAULT NULL,
  `ON_ETSN` int(11) DEFAULT NULL,
  `ON_ECSN` int(11) DEFAULT NULL,
  `ON_PTSN` int(11) DEFAULT NULL,
  `ON_PCSN` int(11) DEFAULT NULL,
  `LIMIT_SPECIAL` varchar(128) DEFAULT NULL,
  `CODE` varchar(128) DEFAULT NULL,
  `REMARK` varchar(512) DEFAULT NULL COMMENT '备注',
  PRIMARY KEY (`id`)
) ENGINE=MyISAM AUTO_INCREMENT=2 DEFAULT CHARSET=utf8;

DROP TABLE IF EXISTS `lptn1_history`;
CREATE TABLE `lptn1_history` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `ENG_SN_USED` varchar(50) DEFAULT NULL,
  `LPTN1_PN` varchar(50) DEFAULT NULL,
  `LPTN1_SN` varchar(50) DEFAULT NULL,
  `ENG_PN` varchar(50) DEFAULT NULL,
  `ON_DATE` int(11) DEFAULT NULL,
  `ON_ETSN` int(11) DEFAULT NULL,
  `ON_ECSN` int(11) DEFAULT NULL,
  `ON_PTSN` int(11) DEFAULT NULL,
  `ON_PCSN` int(11) DEFAULT NULL,
  `OFF_DATE` int(11) DEFAULT NULL,
  `OFF_ETSN` int(11) DEFAULT NULL,
  `OFF_ECSN` int(11) DEFAULT NULL,
  `OFF_PTSN` int(11) DEFAULT NULL,
  `OFF_PCSN` int(11) DEFAULT NULL,
  `LIMIT_SPECIAL` varchar(50) DEFAULT NULL,
  `CODE` varchar(50) DEFAULT NULL,
  `REMARK` varchar(50) DEFAULT NULL,
  PRIMARY KEY (`id`)
) ENGINE=MyISAM AUTO_INCREMENT=75 DEFAULT CHARSET=utf8;

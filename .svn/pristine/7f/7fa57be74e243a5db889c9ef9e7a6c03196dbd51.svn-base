<%@ page contentType="text/html;charset=UTF-8" language="java" %>

<div class="modal fade" id="engfleet-info-modal" tabindex="-1" role="dialog" aria-labelledby="engfleet-info-modal-label">
    <div class="modal-dialog" role="document">
        <div class="box box-primary">
            <div class="box-header with-border">
                <h3 class="box-title">发动机备发信息</h3>
            </div>
            <form id="form-userinfo" method="post" enctype="multipart/form-data">
                <div class="box-body">
                        
                    <div class="form-group">
                        <label for="text-username">ID</label>
                        <input type="tel" class="form-control" id="editid"
                               placeholder="ID" readonly>
                    </div>
                    <div class="form-group">
                        <label for="text-username">签发日期</label>
                        <input type="tel" class="form-control" id="editctime"
                               placeholder="签发日期">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">基地ID</label>
                        <select class="form-control" id="editduty">
                        	<option value="0">请选择</option>
                        	<option value="1">真的是测试用的</option>
                        </select>
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">机型</label>
                        <input type="text" class="form-control" id="editmodle"
                               placeholder="机型">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">机号</label>
                        <input type="text" class="form-control" id="edittail"
                               placeholder="机号">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">发动机位置</label>
                        <input type="text" class="form-control" id="editengPosit"
                               placeholder="发动机位置">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">发动机序号</label>
                        <input type="date" class="form-control" id="editengSn"
                               placeholder="发动机序号">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">ENG_PN</label>
                        <input type="date" class="form-control" id="editengPn"
                               placeholder="ENG_PN">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">Rating</label>
                        <input type="date" class="form-control" id="editrating"
                               placeholder="Rating">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">Config</label>
                        <input type="date" class="form-control" id="editconfigPlane"
                               placeholder="Config">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">LLP余寿</label>
                        <input type="text" class="form-control" id="editllpLifeTime"
                               placeholder="LLP余寿">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">当前ETSN</label>
                        <input type="text" class="form-control" id="editetsn"
                               placeholder="当前ETSN">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">当前ECSN</label>
                        <input type="text" class="form-control" id="editecsn"
                               placeholder="当前ECSN">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">SV_TIME</label>
                        <input type="text" class="form-control" id="editsvTime"
                               placeholder="SV_TIME">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">装机日期</label>
                        <input type="text" class="form-control" id="editinTime"
                               placeholder="装机日期">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">TSI</label>
                        <input type="text" class="form-control" id="edittsi"
                               placeholder="TSI">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">CSI</label>
                        <input type="text" class="form-control" id="editcsi"
                               placeholder="csi">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">TSR</label>
                        <input type="text" class="form-control" id="edittsr"
                               placeholder="tsr">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">CSR</label>
                        <input type="text" class="form-control" id="editcsr"
                               placeholder="CSR">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">TSO</label>
                        <input type="text" class="form-control" id="edittso"
                               placeholder="TSO">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">CSO</label>
                        <input type="text" class="form-control" id="editcso"
                               placeholder="CSO">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">LLP_211_SB门限</label>
                        <input type="text" class="form-control" id="editllp211sb"
                               placeholder="LLP_211_SB门限">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">HPTB余寿</label>
                        <input type="text" class="form-control" id="edithptbLifeTime"
                               placeholder="HPTB余寿">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">ENG_余寿</label>
                        <input type="text" class="form-control" id="editengLifeTime"
                               placeholder="ENG_余寿">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">下发日期_预计</label>
                        <input type="text" class="form-control" id="edituninstallTime"
                               placeholder="下发日期_预计">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">备注</label>
                        <textarea class="form-control" id="editremark" placeholder="备注">
                        </textarea> 
                    </div>
                   
                </div>
                <div class="box-footer">
                    <button type="button" class="btn btn-primary pull-right" onclick="editengfleet()">保存</button>
                </div>
            </form>
        </div>
    </div>
</div>

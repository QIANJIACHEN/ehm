<script>
    var base = "${pageContext.request.contextPath}";
    var storeUrl = null;
    function jumpPage(url) {

        if (storeUrl == url) {
            return;
        }
        document.getElementById("myiframe").src = base + url;
		
        storeUrl = url;
    }
</script>

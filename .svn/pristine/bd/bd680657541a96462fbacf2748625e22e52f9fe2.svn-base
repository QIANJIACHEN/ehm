		return mv;
	}

	/**
	 * 显示消息列表页面
	 */
	@RequestMapping(value = "/news.do")
	public ModelAndView wechatnews() {
		ModelAndView mv = new ModelAndView();
		mv.setViewName("/manage/wechat/news");
		return mv;
	}

	/**
	 * 显示部门列表页面- 通讯录
	 */
	@RequestMapping(value = "/department.do")
	public ModelAndView wechatdepartment() {
		ModelAndView mv = new ModelAndView();

<%@ page contentType="text/html;charset=UTF-8" language="java" %>

<div class="modal fade" id="waterwash-info-modal" tabindex="-1" role="dialog" aria-labelledby="user-info-modal-label">
    <div class="modal-dialog" role="document">
        <div class="box box-primary">
            <div class="box-header with-border">
                <h3 class="box-title">水洗信息</h3>
            </div>
            <form id="form-userinfo" method="post" enctype="multipart/form-data">
                <div class="box-body">
                        
                    <div class="form-group">
                        <label for="text-username">ID</label>
                        <input type="tel" class="form-control" id="editid"
                               placeholder="ID" readonly>
                    </div>
                    <div class="form-group">
                        <label for="text-username">设备参数</label>
                        <input type="tel" class="form-control" id="editparameterApparatus"
                               placeholder="设备参数">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">水洗人员</label>
                        <input type="text" class="form-control" id="edituserId"
                               placeholder="水洗人员">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">地理位置</label>
                        <input type="text" class="form-control" id="editgeographic"
                               placeholder="地理位置">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">天气</label>
                        <input type="text" class="form-control" id="editweather"
                               placeholder="天气">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">发动机型号</label>
                        <input type="text" class="form-control" id="editengSn"
                               placeholder="发动机型号">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">效果</label>
                        <input type="text" class="form-control" id="editresult"
                               placeholder="效果">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">评级</label>
                        <input type="text" class="form-control" id="editlevel"
                               placeholder="评级">
                    </div>
                    <div class="form-group">
                        <label for="text-nickname">持续时间</label>
                        <input type="text" class="form-control form_datetime" id="editdurationTime"
                               placeholder="持续时间">
                    </div>
                </div>
                <div class="box-footer">
                    <button type="button" class="btn btn-primary pull-right" onclick="editwaterwash()">保存</button>
                </div>
            </form>
        </div>
    </div>
</div>

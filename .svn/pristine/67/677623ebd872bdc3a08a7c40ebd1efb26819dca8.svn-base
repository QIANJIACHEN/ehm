	// 获取所有条件id（computation）
	function getCompuIds() {
		var compuIds = [];
		$('.del').each(function(){
			compuIds.push($(this).data('id'));
		});
		return compuIds;
	}

	// 确定按钮点击事件
	function finish(){
		if (getCompuIds().length == 0) {
			alert("最少一个条件！ ");
		}else{
			// 数组转json格式
			var compuIds = JSON.stringify(getCompuIds());
